{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["deno.ns", "deno.worker", "dom", "dom.iterable", "es2022"],
    "strict": false,
    "noImplicitAny": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "exactOptionalPropertyTypes": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": false,
    "noUncheckedIndexedAccess": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "types": ["./_shared/deno-types.d.ts"]
  },
  "imports": {
    "@supabase/supabase-js": "https://esm.sh/@supabase/supabase-js@2.39.3",
    "@google/generative-ai": "https://esm.sh/@google/generative-ai@0.21.0",
    "@shared/types": "./_shared/types.ts",
    "@std/http": "https://deno.land/std@0.168.0/http/server.ts"
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-env --watch index.ts",
    "serve": "supabase functions serve",
    "deploy": "supabase functions deploy"
  }
}
